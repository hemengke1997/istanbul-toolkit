{
  "name": "vite-plugin-istanbul-widget",
  "version": "1.7.1",
  "description": "集成了 istanbul-widget 和 vite-plugin-istanbul 的vite插件",
  "type": "module",
  "keywords": [
    "istanbul",
    "widget",
    "web istanbul",
    "vite-plugin-istanbul",
    "vite-plugin-istanbul-widget"
  ],
  "homepage": "https://github.com/hemengke1997/istanbul-toolkit/tree/master/packages/vite-plugin-istanbul-widget",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/istanbul-toolkit.git",
    "directory": "packages/vite-plugin-istanbul-widget"
  },
  "files": [
    "astro.d.ts",
    "client.d.ts",
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./astro": {
      "types": "./astro.d.ts",
      "require": "./dist/astro.cjs",
      "import": "./dist/astro.js",
      "default": "./dist/astro.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "peerDependencies": {
    "astro": "^3 || ^4",
    "vite": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": true
    }
  },
  "dependencies": {
    "@istanbuljs/load-nyc-config": "1.1.0",
    "@minko-fe/lodash-pro": "^0.3.3",
    "debug": "^4.3.4",
    "espree": "10.0.1",
    "istanbul-lib-instrument": "6.0.2",
    "istanbul-widget": "workspace:*",
    "picocolors": "1.0.0",
    "source-map": "0.7.4",
    "test-exclude": "6.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^20.12.12",
    "@types/serialize-javascript": "5.0.4",
    "serialize-javascript": "6.0.2",
    "vite-plugin-istanbul": "6.0.0"
  }
}
